              var newH = imgH / imgW * newW;
       $('#banner').height(newH);
       $('#banner').css('visibility', 'visible');
      });//end when loaded
  }//end if bg
}//end 
    (function($) {
       $(document).on('ready', resizebanner);
       $(window).on('resize', resizebanner);
    })(jQuery);
</script>



